APP_ENV=production
APP_DEBUG=false
APP_KEY=${APP_KEY}

DB_HOST=${DB_HOST}
DB_PORT=5432
DB_NAME=${DB_NAME}
DB_USER=${DB_USER}
DB_PASS=${DB_PASS}

REDIS_HOST=${REDIS_HOST}
REDIS_PORT=6379
REDIS_PASSWORD=${REDIS_PASSWORD}

RATE_LIMIT=120
RATE_WINDOW=60

JWT_SECRET=${JWT_SECRET}
JWT_EXPIRE=86400

CORS_ORIGIN=https://twod3d-lottery.onrender.com

SECURE_COOKIES=true
SESSION_SECURE=true
HTTPS_ONLY=true

CACHE_DRIVER=redis
SESSION_DRIVER=redis
QUEUE_CONNECTION=redis

LOG_LEVEL=error
ENABLE_CACHE=true 