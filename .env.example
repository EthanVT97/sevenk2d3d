# API ဆက်တင်များ
REACT_APP_API_URL=https://twod3d-lottery-api-q68w.onrender.com

# ဝဘ်ဆိုဒ်ဆက်တင်များ
REACT_APP_SITE_NAME=2D3D ထီ
REACT_APP_SITE_DESCRIPTION=မြန်မာ့ 2D3D ထီဝန်ဆောင်မှု
REACT_APP_SITE_URL=https://twod3d-lottery.onrender.com

# ပတ်ဝန်းကျင်အခြေအနေ
NODE_ENV=production

# လုပ်ဆောင်ချက်များ ဖွင့်/ပိတ်
REACT_APP_ENABLE_2D=true
REACT_APP_ENABLE_3D=true

# အက်ပ်ဆက်တင်များ
REACT_APP_MIN_BET_AMOUNT=100
REACT_APP_MIN_DEPOSIT_AMOUNT=1000
REACT_APP_MAX_WITHDRAW_AMOUNT=1000000

# အချိန်ဆက်တင်များ (မိနစ်ဖြင့်)
REACT_APP_2D_CLOSE_BEFORE=30
REACT_APP_3D_CLOSE_BEFORE=60

# လုံခြုံရေးဆက်တင်များ
REACT_APP_JWT_EXPIRY=86400
REACT_APP_ENABLE_RATE_LIMITING=true 